<div id="contribute">
	<div class="contennt">
		<h1>Sign up</h1>

		<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
		  <%= devise_error_messages! %>

		  <p><%= f.label :email %><br />
		  <%= f.text_field :email, :class => 'big_input' %></p>

		  <p><%= f.label :password %><br />
		  <%= f.password_field :password, :class => 'big_input' %></p>

		  <p><%= f.label :password_confirmation %><br />
		  <%= f.password_field :password_confirmation, :class => 'big_input' %></p>

		  <p><%= f.submit "Sign up" %></p>
		<% end %>

		<%= render :partial => "devise/shared/links" %>
		
	</div>
</div>